{
  "presets": [
    [
      "@babel/preset-env", {
        "useBuiltIns": "entry",
        "corejs": "3.0.0"
      }
    ],
    "@babel/preset-react"],
    "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "regenerator": true
          }
        ],
        [
            "babel-plugin-styled-components",
            {
                "displayName": true,
                "ssr": true,
                "minify": false,
                "transpileTemplateLiterals": false
            }
        ]
    ]
}
