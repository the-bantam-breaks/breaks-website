{
  "name": "breaks-website",
  "version": "0.0.1",
  "description": "Official Website for the Atlanta Rock Band The Bantam Breaks",
  "main": "index.js",
  "repository": "git@github.com:the-bantam-breaks/breaks-website.git",
  "author": "Scotland Stephenson <scotlandrummer@yahoo.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint ./src",
    "start": "webpack -w & nodemon server.js",
    "dev": "nodemon",
    "startup": "node ./build/server.js",
    "start-local-db": "docker run -d --rm -p 28015:28015 -p 8090:8080 -v \"$PWD/data:/data\" --name bantam_rethinkdb rethinkdb",
    "stop-local-db": "docker stop bantam_rethinkdb",
    "start:server": "rm -rf ./build && webpack --config webpack.config.server.js",
    "debugkoa": "DEBUG=koa-send* node .build/server.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@hapi/boom": "^7.4.2",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-mount": "^4.0.0",
    "koa-route": "^3.2.0",
    "koa-static": "^5.0.0",
    "nodemon": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-sticky": "^6.0.3",
    "request": "2.88.0",
    "rethinkdb": "^2.3.3",
    "start-server-webpack-plugin": "^2.2.5",
    "styled-components": "^4.3.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.0"
  }
}
